{
  "name": "angular-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angular-playground && ng build angular-playground-mfe",
    "build:localhost": "npm run lint:all && npm run test && ng build angular-playground --base-href=/angular-playground/ && ng build angular-playground-mfe --base-href=/angular-playground-mfe/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint \"apps/**/src/**/*.scss\" --config stylelint.config.js",
    "lint:styles:fix": "stylelint \"apps/**/src/**/*.scss\" --config stylelint.config.js --fix",
    "lint:all": "npm run lint && npm run lint:styles",
    "lint:all:fix": "npm run lint:fix && npm run lint:styles:fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.2",
    "@angular/cdk": "^20.0.2",
    "@angular/common": "^20.0.2",
    "@angular/compiler": "^20.0.2",
    "@angular/core": "^20.0.2",
    "@angular/forms": "^20.0.2",
    "@angular/material": "^20.0.2",
    "@angular/platform-browser": "^20.0.2",
    "@angular/platform-browser-dynamic": "^20.0.2",
    "@angular/platform-server": "^20.0.2",
    "@angular/router": "^20.0.2",
    "@angular/ssr": "^20.0.1",
    "@ngxs/storage-plugin": "^19.0.0",
    "@ngxs/store": "^19.0.0",
    "@softarc/native-federation-node": "^2.0.10",
    "es-module-shims": "^1.5.12",
    "highlight.js": "^11.11.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^20.0.1",
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular-devkit/core": "^20.0.1",
    "@angular-devkit/schematics": "^20.0.1",
    "@angular-eslint/builder": "^20.0.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular-eslint/eslint-plugin-template": "^20.0.0",
    "@angular-eslint/schematics": "^20.0.0",
    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.2",
    "@angular/language-service": "^20.0.2",
    "@types/jasmine": "~5.1.8",
    "angular-eslint": "20.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jasmine-core": "~5.7.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "msw": "^2.7.6",
    "ng-mocks": "^14.13.5",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "stylelint": "^16.20.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "msw": {
    "workerDirectory": [
      "apps/angular-playground/src"
    ]
  }
}
