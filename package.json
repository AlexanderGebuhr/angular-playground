{
  "name": "angular-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angular-playground && ng build angular-playground-mfe",
    "build:localhost": "npm run lint:all && npm run test && ng build angular-playground --base-href=/angular-playground/ && ng build angular-playground-mfe --base-href=/angular-playground-mfe/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint \"apps/**/src/**/*.scss\" --config stylelint.config.js",
    "lint:styles:fix": "stylelint \"apps/**/src/**/*.scss\" --config stylelint.config.js --fix",
    "lint:all": "npm run lint && npm run lint:styles",
    "lint:all:fix": "npm run lint:fix && npm run lint:styles:fix"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/native-federation": "21.0.2",
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^21.0.2",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^21.0.2",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@fontsource/material-symbols-outlined": "^5.2.30",
    "@fontsource/roboto": "^5.2.9",
    "@ngxs/storage-plugin": "^20.1.0",
    "@ngxs/store": "^20.1.0",
    "es-module-shims": "1.10.1",
    "highlight.js": "^11.11.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.2",
    "@angular-devkit/core": "^21.0.2",
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@angular/language-service": "^21.0.3",
    "@types/jasmine": "~5.1.13",
    "angular-eslint": "21.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "msw": "2.7.6",
    "ng-mocks": "^14.14.0",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "stylelint": "^16.26.1",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "msw": {
    "workerDirectory": [
      "apps/angular-playground/src"
    ]
  }
}
